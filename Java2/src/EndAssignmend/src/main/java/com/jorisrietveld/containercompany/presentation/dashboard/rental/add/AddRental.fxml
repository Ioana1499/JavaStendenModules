<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Tab text="Add New Rental"
     fx:controller="Gui.Rental.RentalController"
     xmlns="http://javafx.com/javafx/8.0.141"
     xmlns:fx="http://javafx.com/fxml/1">
  <!-- This is the component responsible for adding new container rentals in the rental manager. -->
  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <ChoiceBox layoutX="39.0"
               layoutY="58.0"
               prefHeight="25.0"
               prefWidth="626.0"
               AnchorPane.leftAnchor="39.0"
               AnchorPane.rightAnchor="36.0"/>
    <Label layoutX="40.0"
           layoutY="16.0"
           prefHeight="34.0"
           prefWidth="628.0"
           text="Select an container"
           textAlignment="CENTER"
           textOverrun="CENTER_ELLIPSIS"/>
    <ListView layoutX="40.0"
              layoutY="101.0"
              prefHeight="180.0"
              prefWidth="625.0"
              AnchorPane.leftAnchor="40.0"
              AnchorPane.rightAnchor="36.0"/>
    <Label layoutX="40.0"
           layoutY="293.0"
           prefHeight="34.0"
           prefWidth="307.0"
           text="Select an start date"
           textAlignment="CENTER"
           textOverrun="CENTER_ELLIPSIS"/>

    <DatePicker layoutX="40.0" layoutY="341.0" prefHeight="25.0" AnchorPane.leftAnchor="40.0"/>
    <DatePicker layoutX="359.0" layoutY="341.0" prefHeight="25.0" AnchorPane.rightAnchor="40.0"/>
  <Label layoutX="360.0"
         layoutY="293.0"
         prefHeight="34.0"
         prefWidth="305.0"
         text="Select an end date"
         textAlignment="CENTER"
         textOverrun="CENTER_ELLIPSIS"/>
  <Button layoutX="40.0"
          layoutY="388.0"
          mnemonicParsing="false"
          prefHeight="30.0"
          prefWidth="623.0"
          text="Add Rental"
          textAlignment="CENTER"
          AnchorPane.leftAnchor="40.0"
          AnchorPane.rightAnchor="40.0"/>
</AnchorPane>
  </Tab>
