<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox prefHeight="529.0" prefWidth="164.0" BorderPane.alignment="CENTER">
    <children>
        <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#showRentalTabs" prefHeight="116.0" prefWidth="164.0" text="Rental Mananger">
            <graphic>
                <ImageView fitHeight="70.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../Resources/Icons/handshake.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button contentDisplay="TOP" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#showContainerTabs" prefHeight="116.0" prefWidth="164.0" text="Container Manager">
            <graphic>
                <ImageView fitHeight="70.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../Resources/Icons/package.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button contentDisplay="TOP" layoutX="10.0" layoutY="126.0" mnemonicParsing="false" onAction="#showStatisticsTabs" prefHeight="116.0" prefWidth="164.0" text="Statistics">
            <graphic>
                <ImageView fitHeight="70.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../Resources/Icons/bar-chart.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button contentDisplay="TOP" layoutX="10.0" layoutY="242.0" mnemonicParsing="false" prefHeight="116.0" prefWidth="164.0" text="Settings">
            <graphic>
                <ImageView fitHeight="70.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../Resources/Icons/settings-2.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</VBox>