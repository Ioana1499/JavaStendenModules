<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>

<Tab text="Add Container"
  fx:controller="com.jorisrietveld.containercompany.presentation.dashboard.container.AddContainerController"
     xmlns="http://javafx.com/javafx/8.0.141"
     xmlns:fx="http://javafx.com/fxml/1">
  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <Slider fx:id="widthSlider" layoutX="41.0" layoutY="132.0" prefHeight="14.0" prefWidth="626.0"
            AnchorPane.leftAnchor="41.0" AnchorPane.rightAnchor="34.0"/>

    <Label layoutX="247.0" layoutY="95.0" prefHeight="17.0" prefWidth="170.0" text="Container Width:"
           textAlignment="CENTER"/>

    <Slider fx:id="lengthSlider" layoutX="39.0" layoutY="203.0" prefHeight="14.0" prefWidth="626.0"
            AnchorPane.leftAnchor="39.0" AnchorPane.rightAnchor="36.0"/>

    <Label layoutX="244.0" layoutY="160.0" prefHeight="17.0" prefWidth="170.0" text="Container Length:"
           textAlignment="CENTER"/>

    <Slider fx:id="heightSlider" layoutX="40.0" layoutY="269.0" prefHeight="14.0" prefWidth="626.0"
            AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="35.0"/>

    <Label layoutX="246.0" layoutY="238.0" prefHeight="17.0" prefWidth="170.0" text="Container Height:"
           textAlignment="CENTER"/>


    <ChoiceBox layoutX="33.0" layoutY="48.0" prefHeight="25.0" prefWidth="535.0" fx:id="containerTypeChoiceBox"/>

    <Label layoutX="240.0" layoutY="14.0" prefHeight="17.0" prefWidth="170.0" text="Container Type:"
           textAlignment="CENTER"/>
    <Button layoutX="34.0" layoutY="319.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="545.0"
            text="Add Container" onAction="#addContainer"/>
  </AnchorPane>
</Tab>
